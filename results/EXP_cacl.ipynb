   "source": [
    "def calculate_S(alpha, gamma, c, b):\n",
    "    numerator = 1 - alpha ** (gamma + 1)\n",
    "    denominator = (1 - alpha) * (c * gamma + b + 1)\n",
    "    S = numerator / denominator\n",
    "    return S"
   ]
  },
